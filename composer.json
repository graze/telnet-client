    "require": {
        "php": ">5.5.0",
        "clue/socket-raw": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "^5.0",
        "symfony/var-dumper": "^3.0",
        "mockery/mockery": "^0.9.4",
        "squizlabs/php_codesniffer": "^3.5.0",
        "graze/standards": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Graze\\TelnetClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Graze\\TelnetClient\\Test\\Unit\\": "tests/unit"
        }
    },
    "config": {
        "platform": {
            "ext-simplexml": "1.0",
            "ext-dom": "1.0",
            "ext-tokenizer": "1.0",
            "ext-xmlwriter": "1.0",
            "ext-sockets": "1.0",
            "php": "5.6"
        }
    }
